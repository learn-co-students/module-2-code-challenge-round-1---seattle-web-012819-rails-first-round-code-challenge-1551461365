This is the search page!
<h2>All Heroines</h2>

<%= form_tag("/search", method: "get") do %>
  <b><%= label_tag(:power_id, "Search By Superpower:") %></b><br>
  <%= text_field_tag(:power_id) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
