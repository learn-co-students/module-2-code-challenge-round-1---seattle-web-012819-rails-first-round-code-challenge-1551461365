<h2>All Heroines</h2>

<div id = "search">
  <%= form_tag(heroines_path, method: :get) do %>
  <h5>Seach Super Powers</h5> <%= text_field_tag(:term, params[:term]) %>
  <%= submit_tag 'Search' %>
  <% end %>
</div>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
