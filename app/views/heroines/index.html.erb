<%= image_tag("https://ap2hyc.com/wp-content/uploads/2014/01/Marvel.lady_.superheroes1.jpg") %>

<h2>List of Heroines</h2>

<%= form_tag(heroines_path, method: :get) do %>
  <%= label_tag(:search, "Search by Super Power!") %>
  <%= text_field_tag(:search, params[:search]) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>

<h2><%= link_to 'Create a new Heroine!', new_heroine_path %> </h2>
